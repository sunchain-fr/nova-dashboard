{
    "name": "sunchain/nova-dashboard",
    "description": "The missing dashboard for nova.",
    "keywords": [
        "laravel",
        "nova",
        "dashboard",
        "widgets",
        "flexible"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "divineomega/array_undot": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\NovaDashboard\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\NovaDashboard\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
